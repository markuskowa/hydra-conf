{
  "master-unstable": {
    "checkinterval": 43200,
    "description": "Build master against nixpkgs-unstable",
    "emailoverride": "",
    "enabled": true,
    "enableemail": false,
    "hidden": false,
    "inputs": {
      "config": {
        "emailresponsible": false,
        "type": "nix",
        "value": "{ optAVX = true; }"
      },
      "allowUnfree": {
        "emailresponsible": false,
        "type": "boolean",
        "value": "false"
      },
      "pin": {
        "emailresponsible": false,
        "type": "boolean",
        "value": "true"
      },
      "postOverlays": {
	"type": "nix",
	"value": "[(self: super: { qchem=super.qchem // { amd-scalapack = null; }; })]"
      },
      "nixpkgs": {
        "emailresponsible": false,
        "type": "git",
        "value": "https://github.com/NixOS/nixpkgs nixpkgs-unstable"
      },
      "qchem": {
        "emailresponsible": false,
        "type": "git",
        "value": "https://github.com/markuskowa/NixOS-QChem master"
      }
    },
    "keepnr": 5,
    "nixexprinput": "qchem",
    "nixexprpath": "release.nix",
    "schedulingshares": 100
  },
