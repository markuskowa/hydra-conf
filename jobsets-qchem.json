{
  "master-unstable": {
    "checkinterval": 86400,
    "description": "Build master against nixpkgs-unstable",
    "emailoverride": "",
    "enabled": true,
    "enableemail": false,
    "hidden": false,
    "inputs": {
      "config": {
        "emailresponsible": false,
        "type": "nix",
        "value": "{ optAVX = true; }"
      },
      "allowUnfree": {
        "emailresponsible": false,
        "type": "boolean",
        "value": "false"
      },
      "preOverlays": {
        "emailresponsible": false,
        "type": "nix",
        "value": "[(self: super: { openblas = super.openblas.overrideAttrs (x: { makeFlags = x.makeFlags ++ [ \"NO_AVX512=1\" ]; }); })]"
      },
      "nixpkgs": {
        "emailresponsible": false,
        "type": "git",
        "value": "git://github.com/NixOS/nixpkgs.git nixpkgs-unstable"
      },
      "qchem": {
        "emailresponsible": false,
        "type": "git",
        "value": "git://github.com/markuskowa/NixOS-QChem.git master"
      }
    },
    "keepnr": 5,
    "nixexprinput": "qchem",
    "nixexprpath": "release.nix",
    "schedulingshares": 100
  },
  "master-staging": {
    "checkinterval": 0,
    "description": "Build openblas/branch against nixpkgs staging",
    "emailoverride": "",
    "enabled": true,
    "enableemail": false,
    "hidden": false,
    "inputs": {
      "config": {
        "emailresponsible": false,
        "type": "nix",
        "value": "{ optAVX = true; }"
      },
      "allowUnfree": {
        "emailresponsible": false,
        "type": "boolean",
        "value": "false"
      },
      "nixpkgs": {
        "emailresponsible": false,
        "type": "git",
        "value": "git://github.com/NixOS/nixpkgs.git staging"
      },
      "qchem": {
        "emailresponsible": false,
        "type": "git",
        "value": "git://github.com/markuskowa/NixOS-QChem.git openblas"
      }
    },
    "keepnr": 1,
    "nixexprinput": "qchem",
    "nixexprpath": "release.nix",
    "schedulingshares": 100
  },
  "master-unstable-noavx": {
    "checkinterval": 172800,
    "description": "Build master against nixpkgs-unstable (AVX optimizations disabled)",
    "emailoverride": "",
    "enabled": true,
    "enableemail": false,
    "hidden": false,
    "inputs": {
      "config": {
        "emailresponsible": false,
        "type": "nix",
        "value": "{ optAVX = false; }"
      },
      "allowUnfree": {
        "emailresponsible": false,
        "type": "boolean",
        "value": "false"
      },
      "nixpkgs": {
        "emailresponsible": false,
        "type": "git",
        "value": "git://github.com/NixOS/nixpkgs.git nixpkgs-unstable"
      },
      "qchem": {
        "emailresponsible": false,
        "type": "git",
        "value": "git://github.com/markuskowa/NixOS-QChem.git master"
      }
    },
    "keepnr": 5,
    "nixexprinput": "qchem",
    "nixexprpath": "release.nix",
    "schedulingshares": 100
  },
  "release-20.09": {
    "checkinterval": 0,
    "description": "Build release-20.09 against nixos-20.09",
    "emailoverride": "",
    "enabled": true,
    "enableemail": false,
    "hidden": false,
    "inputs": {
      "config": {
        "emailresponsible": false,
        "type": "nix",
        "value": "{ optAVX = true; }"
      },
      "allowUnfree": {
        "emailresponsible": false,
        "type": "boolean",
        "value": "false"
      },
      "nixpkgs": {
        "emailresponsible": false,
        "type": "git",
        "value": "git://github.com/NixOS/nixpkgs.git nixos-20.09"
      },
      "qchem": {
        "emailresponsible": false,
        "type": "git",
        "value": "git://github.com/markuskowa/NixOS-QChem.git release-20.09"
      }
    },
    "keepnr": 1,
    "nixexprinput": "qchem",
    "nixexprpath": "release.nix",
    "schedulingshares": 100
  }
}
